<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="com.darski.SimpleTimer.controller.MainController">
    <children>
        <HBox alignment="CENTER" prefHeight="48.0" prefWidth="600.0" spacing="20.0">
            <children>
                <Label prefHeight="22.0" prefWidth="144.0" text="Type timer's name:" textAlignment="CENTER"/>
                <TextField fx:id="timerName" prefHeight="32.0" prefWidth="202.0" promptText="Timer" text="Timer"/>
                <Button mnemonicParsing="false" onAction="#addTimer" text="Add!"/>
            </children>
        </HBox>
        <VBox fx:id="timersVBox" alignment="CENTER" layoutX="14.0" layoutY="66.0">
            <padding>
                <Insets top="20.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
